  <appSettings>
    <add key="DebugMode" value="1"/>
    <add key="DisableSPM" value="1"/>
    <add key="PageSize" value="30"/>
    <add key="AccessRightManager" value="WebAccessRightManager"/>
    <add key="ControlsPath" value="/_controltemplates/onestop"/>
  </appSettings>
  <connectionStrings>
    <add name="ConnectionString" connectionString="Data Source=(local);Initial Catalog=contextum2;Integrated Security=True"
      providerName="System.Data.SqlClient" />
    <add name="licwebConnectionString" connectionString="Data Source=(local);Initial Catalog=licweb;Integrated Security=True"
      providerName="System.Data.SqlClient" />
    <add name="ermsConnectionString" connectionString="Data Source=srvsql\mssqlserver2008;Initial Catalog=erms;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.web>
    <compilation debug="false">
      <assemblies>
        <add assembly="System.Data.Linq, Version=3.5.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
        <add assembly="System.Windows.Forms, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089"/>
