<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net5.0;net6.0</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Tango.UI.Std\Tango.UI.Std.csproj" />
    <ProjectReference Include="..\Tango\Tango.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Autogenerated\generate.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
      <LastGenOutput>generate.cs</LastGenOutput>
    </Content>
    <Content Include="Meta\compile.tt">
      <Generator>TextTemplatingFileGenerator</Generator>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Autogenerated\generate.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>generate.tt</DependentUpon>
    </Compile>
    <Compile Update="Meta\compile.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>compile.tt</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>