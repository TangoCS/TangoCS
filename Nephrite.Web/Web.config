<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Nephrite.Web.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <connectionStrings>
    <add name="ConnectionString" connectionString="Data Source=(local);Initial Catalog=Fund2;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="Fund2ConnectionString" connectionString="Data Source=(local)\v2008;Initial Catalog=Fund2;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="oppsConnectionString" connectionString="Data Source=(local);Initial Catalog=opps;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="ContextumConnectionString" connectionString="Data Source=(local);Initial Catalog=Contextum;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="oppswebConnectionString" connectionString="Data Source=(local);Initial Catalog=oppsweb;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="ContextumConnectionString1" connectionString="Data Source=(local);Initial Catalog=Contextum;Integrated Security=True;Connect Timeout=150" providerName="System.Data.SqlClient" />
    <add name="licwebConnectionString" connectionString="Data Source=(local);Initial Catalog=licweb;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="opps2ConnectionString" connectionString="Data Source=srvsql\mssqlserver2008;Initial Catalog=opps2;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="opps3ConnectionString" connectionString="Data Source=srvsql\mssqlserver2008;Initial Catalog=opps3;Integrated Security=True" providerName="System.Data.SqlClient" />
    <add name="contextum2ConnectionString" connectionString="Data Source=srvsql\mssqlserver2008;Initial Catalog=contextum2;Integrated Security=True" providerName="System.Data.SqlClient" />
